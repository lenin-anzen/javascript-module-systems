{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "rules": {
        "no-invalid-this": "off",
        "no-new-object": "off",
        "require-jsdoc": "off",
        "prefer-rest-params": "off"
    }
}
